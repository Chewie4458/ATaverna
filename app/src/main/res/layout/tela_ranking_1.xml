<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

  <ImageView
      android:id="@+id/bkgTelaRanking"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:background="@drawable/container_ranking_rectangle"
      android:scaleType="centerCrop"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0" />

  <ImageView
      android:id="@+id/imgAlbumRanking"
      android:layout_width="266.67dp"
      android:layout_height="266.67dp"
      android:background="@drawable/container_ranking_rectangle1"
      android:scaleType="centerInside"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.502"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="@+id/bkgTelaRanking"
      app:layout_constraintVertical_bias="0.075" />

  <ImageView
      android:id="@+id/imgComentarioRanking"
      android:layout_width="332dp"
      android:layout_height="202dp"
      android:layout_marginBottom="32dp"
      android:background="@drawable/container_ranking_rectangle2"
      android:scaleType="centerInside"
      app:layout_constraintBottom_toBottomOf="@+id/bkgTelaRanking"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.505"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imgAlbumRanking"
      app:layout_constraintVertical_bias="1.0" />

  <ImageButton
      android:id="@+id/btnVoltarRanking"
      android:layout_width="33dp"
      android:layout_height="24dp"
      android:background="#00FFFFFF"
      android:scaleType="fitCenter"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.039"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.016"
      app:srcCompat="@drawable/container_ranking_vector1" />

  <ImageButton
      android:id="@+id/btnEstrelaRanking_3"
      android:layout_width="55dp"
      android:layout_height="55dp"
      android:layout_marginTop="73dp"
      android:layout_marginBottom="123dp"
      android:background="#00FFFFFF"
      android:onClick="btnEstrelaRanking_3"
      android:scaleType="fitCenter"
      app:layout_constraintBottom_toTopOf="@+id/imgComentarioRanking"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imgAlbumRanking"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/ic_star_outline1" />

  <ImageButton
      android:id="@+id/btnEstrelaRanking_2"
      android:layout_width="55dp"
      android:layout_height="55dp"
      android:layout_marginTop="73dp"
      android:layout_marginBottom="123dp"
      android:background="#00FFFFFF"
      android:onClick="btnEstrelaRanking_2"
      android:scaleType="fitCenter"
      app:layout_constraintBottom_toTopOf="@+id/imgComentarioRanking"
      app:layout_constraintEnd_toStartOf="@+id/btnEstrelaRanking_3"
      app:layout_constraintTop_toBottomOf="@+id/imgAlbumRanking"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/ic_star_outline1" />

  <ImageButton
      android:id="@+id/btnEstrelaRanking_4"
      android:layout_width="55dp"
      android:layout_height="55dp"
      android:layout_marginTop="73dp"
      android:layout_marginBottom="123dp"
      android:background="#00FFFFFF"
      android:onClick="btnEstrelaRanking_4"
      android:scaleType="fitCenter"
      app:layout_constraintBottom_toTopOf="@+id/imgComentarioRanking"
      app:layout_constraintStart_toEndOf="@+id/btnEstrelaRanking_3"
      app:layout_constraintTop_toBottomOf="@+id/imgAlbumRanking"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/ic_star_outline1" />

  <ImageButton
      android:id="@+id/btnEstrelaRanking_1"
      android:layout_width="55dp"
      android:layout_height="55dp"
      android:layout_marginTop="73dp"
      android:layout_marginBottom="123dp"
      android:background="#00FFFFFF"
      android:backgroundTintMode="add"
      android:foregroundGravity="fill"
      android:onClick="btnEstrelaRanking_1"
      android:scaleType="fitCenter"
      app:layout_constraintBottom_toTopOf="@+id/imgComentarioRanking"
      app:layout_constraintEnd_toStartOf="@+id/btnEstrelaRanking_2"
      app:layout_constraintHorizontal_bias="1.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imgAlbumRanking"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/ic_star_outline1" />

  <ImageButton
      android:id="@+id/btnEstrelaRanking_5"
      android:layout_width="55dp"
      android:layout_height="55dp"
      android:layout_marginTop="73dp"
      android:layout_marginBottom="123dp"
      android:background="#00FFFFFF"
      android:onClick="btnEstrelaRanking_5"
      android:scaleType="fitCenter"
      app:layout_constraintBottom_toTopOf="@+id/imgComentarioRanking"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toEndOf="@+id/btnEstrelaRanking_4"
      app:layout_constraintTop_toBottomOf="@+id/imgAlbumRanking"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/ic_star_outline1" />

  <TextView
      android:id="@+id/lblAlbumNome"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:fontFamily="@font/gemunu_libre_bold"
      android:text="XXX"
      android:textColor="@color/white"
      android:textSize="24sp"
      app:layout_constraintBottom_toTopOf="@+id/btnEstrelaRanking_3"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imgAlbumRanking" />

  <EditText
      android:id="@+id/edtComentario"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:ems="10"
      android:fontFamily="@font/gemunu_libre_bold"
      android:gravity="start|top"
      android:inputType="textMultiLine"
      android:singleLine="false"
      android:textColor="@color/white"
      app:layout_constraintBottom_toBottomOf="@+id/imgComentarioRanking"
      app:layout_constraintEnd_toEndOf="@+id/imgComentarioRanking"
      app:layout_constraintStart_toStartOf="@+id/imgComentarioRanking"
      app:layout_constraintTop_toTopOf="@+id/imgComentarioRanking" />

</androidx.constraintlayout.widget.ConstraintLayout>
